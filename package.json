{
  "name": "node-js-getting-started",
  "version": "0.2.5",
  "description": "A sample Node.js app using Express 4",
  "engines": {
    "node": "5.9.1"
  },
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./index.js --exec babel-node"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "casual": "^1.5.3",
    "ejs": "2.4.1",
    "express": "4.13.3",
    "express-graphql": "^0.5.3",
    "graphql": "^0.5.0",
    "graphql-relay": "^0.4.2",
    "graphql-tools": "^0.4.2",
    "lodash": "^4.14.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-relay": "^0.9.2",
    "sequelize": "^3.23.6",
    "sqlite3": "^3.1.4"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-polyfill": "6.5.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-relay-plugin": "^0.9.2",
    "babel-preset-stage-0": "6.5.0",
    "babelify": "^7.3.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-import": "^1.1.0",
    "eslint-plugin-react": "^4.2.3",
    "nodemon": "^1.10.0",
    "onchange": "^2.5.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku/node-js-getting-started"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
